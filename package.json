    "express": "^4.17.2",
    "node-fetch": "^2.0.0",
    "socket.io": "^4.4.1",
    "socket.io-client": "^4.4.1"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.1.0",
    "@types/elliptic": "^6.4.14",
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.13",
    "@types/node-fetch": "^2.5.12",
    "nodemon": "^2.0.15",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.5"
  }
